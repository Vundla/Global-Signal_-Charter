{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "lib": ["ES2020", "DOM", "WebWorker"],
    "moduleResolution": "bundler",
    "module": "ESNext",
    "target": "ES2020"
  }
}
